# Build a docker image

$ docker build -t docker-hadoop-3.2.2 -f Dockerfile .

# tag and push it to container registry of adk2-mr

$ docker tag docker-hadoop-3.2.2:latest us.gcr.io/trans-mind-749/docker-hadoop-3.2.2:latest



$ gcloud auth print-access-token | docker login -u oauth2accesstoken --password-stdin https://us.gcr.io
$ docker push us.gcr.io/trans-mind-749/docker-hadoop-3.2.2:latest

